{
  "name": "opuscodium-odoo",
  "version": "1.0.1",
  "author": "Opus Codium",
  "summary": "Installs, configures, and manages the Odoo ERP and CRM",
  "license": "Apache-2.0",
  "source": "https://github.com/opus-codium/puppet-odoo",
  "project_page": "https://github.com/opus-codium/puppet-odoo",
  "issues_url": "https://github.com/opus-codium/puppet-odoo/issues",
  "dependencies": [
    {
      "name": "puppet-archive",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 8.0.0"
    }
  ]
}
