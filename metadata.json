{
  "name": "opuscodium-odoo",
  "version": "5.1.1-rc0",
  "author": "Opus Codium",
  "summary": "Installs, configures, and manages the Odoo ERP and CRM",
  "license": "Apache-2.0",
  "source": "https://github.com/opus-codium/puppet-odoo",
  "project_page": "https://github.com/opus-codium/puppet-odoo",
  "issues_url": "https://github.com/opus-codium/puppet-odoo/issues",
  "dependencies": [
    {
      "name": "puppet-archive",
      "version_requirement": ">= 4.0.0 < 8.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 1.0.0 < 11.0.0"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": ">= 4.0.0 < 7.0.0"
    },
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 9.0.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "22.04",
        "24.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">= 8.19.0 < 9.0.0"
    }
  ]
}
