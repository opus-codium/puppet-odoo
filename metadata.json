{
  "name": "opuscodium-odoo",
  "version": "1.1.0",
  "author": "Opus Codium",
  "summary": "Installs, configures, and manages the Odoo ERP and CRM",
  "license": "Apache-2.0",
  "source": "https://github.com/opus-codium/puppet-odoo",
  "project_page": "https://github.com/opus-codium/puppet-odoo",
  "issues_url": "https://github.com/opus-codium/puppet-odoo/issues",
  "dependencies": [
    {
      "name": "puppet-archive",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": "< 8.0.0"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.13.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "18.04",
        "20.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ]
}
